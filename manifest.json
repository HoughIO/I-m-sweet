{
"manifest_version": 2,
    "name": "I'm Sweet",
    "description": "Sweetens all your web pages.",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}
